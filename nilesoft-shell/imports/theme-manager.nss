menu(menu=name_menu+'/Gradient/color-2'						type='taskbar' expanded=1) {
	$is_ch=ini.get(path_nsi, name_sct, 'theme.background.gradient.stop')
	$array_stops	=regex.matches(is_ch, '(\[\s*)([01](?:\.\d{1,5})?)(?=\s*,)')
	$array_colors	=regex.matches(is_ch, '(,\s*)([^,\]\[]+)(?=\s*,)')
	$array_opacities=regex.matches(is_ch, '(,\s*)(\b([1-9]\d?|100)\b)(?=\s*\]\s*)')
	$array_ratios	=regex.matches(is_ch, '(\/\/\s*)(\d{2}:\d{2}:\d{2})')
	$array_trans	=regex.matches(is_ch, '(#\s*)(\b([0-9]\d?|100)\b)')				

	$ratio=str.trim(str.sub(array_ratios[0],2))
	$trans=str.trim(str.sub(array_trans[0],1))
	$se11b=str.trim(str.sub(array_stops[0],1))
	$se12b=str.trim(str.sub(array_colors[0],1))
	$se13b=str.trim(str.sub(array_opacities[0],1))
	$se21b=str.trim(str.sub(array_stops[1],1))
	$se22b=if(trans==100 or trans=='', str.trim(str.sub(array_colors[1],1)),		str.trim(str.sub(array_colors[3],1)))
	$se23b=if(trans==100 or trans=='', str.trim(str.sub(array_opacities[1],1)),	str.trim(str.sub(array_opacities[3],1)))
	$se31b=str.trim(str.sub(array_stops[2],1))
	$se32b=if(trans==100 or trans=='', str.trim(str.sub(array_colors[2],1)),		str.trim(str.sub(array_colors[6],1)))
	$se33b=if(trans==100 or trans=='', str.trim(str.sub(array_opacities[2],1)),	str.trim(str.sub(array_opacities[6],1)))

	$sv='[[@se11b, @se12b, @se13b]@if(trans!=100 and trans!='', ', [(@se11b+(@se21b-@se11b)/2*(1-@trans/100)), @se12b, @se13b], [(@se21b-(@se21b-@se11b)/2*(1-@trans/100)), #######, @se23b]'), [@se21b, #######, @se23b]'
		+'@if(se31b!='', '@if(trans!=100 and trans!='', ', [(@se21b+(@se31b-@se21b)/2*(1-@trans/100)), #######, @se23b], [(@se31b-(@se31b-@se21b)/2*(1-@trans/100)), @se32b, @se33b]'), [@se31b, @se32b, @se33b]')]//@if(ratio!='', '@(ratio)')@if(trans!='', '#@(trans)')'
