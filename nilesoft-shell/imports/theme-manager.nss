menu(menu=name_menu+'/image/color-2'						type='taskbar' expanded=1) {
	$th_image_color=ini.get(path_nsi, name_sct, 'theme.image.color')
	$th_image_c1 = if(th_image_color=='', 'image.color1', str.trim(str.sub(th_image_color, +1, str.find(th_image_color, ',')-1)))
	$th_image_c2 = if(th_image_color=='', 'image.color2', str.trim(str.sub(th_image_color, +1+ str.find(th_image_color, ','), str.findlast(th_image_color, ',')-str.find(th_image_color, ',')-1)))
	$th_image_c3 = if(th_image_color=='', 'image.color3', str.trim(str.sub(th_image_color, +1+ str.findlast(th_image_color, ','), str.len(th_image_color)-str.findlast(th_image_color, ',')-1-1)))
	item(title='system@ds' 		checked=th_image_c2=='image.color2'											cmd=ini.set(path_nsi, name_sct, 'theme.image.color', '[@th_image_c1, image.color2, @th_image_c3]') & cmd_crate)
